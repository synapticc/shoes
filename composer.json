{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "barbieswimcrew/zip-code-validator": "^3.1.5",
        "beberlei/doctrineextensions": "^1.5",
        "doctrine/doctrine-bundle": ">=3.2.2",
        "doctrine/doctrine-migrations-bundle": ">=4",
        "doctrine/orm": ">=3.6.2",
        "donatj/phpuseragentparser": "^1.11",
        "foroco/php-browser-detection": "^2.8",
        "gedmo/doctrine-extensions": "^3.22",
        "giggsey/libphonenumber-for-php": "^9.0.23",
        "jasny/twig-extensions": "^1.3.1",
        "jawira/case-converter": "^3.6",
        "jlorente/php-credit-cards": "^1.0.4",
        "knplabs/knp-paginator-bundle": "^6.10",
        "knplabs/knp-time-bundle": "^2.5",
        "league/html-to-markdown": "^5.1.1",
        "loilo/fuse": "^7.1.1",
        "lucascavalcante/fuzzy-search": "^1.0.1",
        "martin-georgiev/postgresql-for-doctrine": "^4.1",
        "minwork/array": "^1.13.5",
        "nzo/url-encryptor-bundle": "^6.5",
        "odolbeau/phone-number-bundle": "^4.2.5",
        "opsway/doctrine-dbal-postgresql": "^2.1",
        "oro/doctrine-extensions": ">=3",
        "phpdocumentor/reflection-docblock": "^5.6.6",
        "phpstan/phpdoc-parser": ">=2.3.2",
        "qipsius/tcpdf-bundle": ">=4",
        "rinvex/countries": ">=9.1",
        "scienta/doctrine-json-functions": ">=6.3",
        "spatie/image-optimizer": "^1.8.1",
        "spatie/pdf-to-image": "^3.1",
        "stof/doctrine-extensions-bundle": "^1.15.3",
        "symfony/apache-pack": ">=1.0.1",
        "symfony/asset": "8.0.*",
        "symfony/console": "8.0.*",
        "symfony/doctrine-messenger": "8.0.*",
        "symfony/dotenv": "8.0.*",
        "symfony/error-handler": "8.0.*",
        "symfony/expression-language": "8.0.*",
        "symfony/filesystem": "8.0.*",
        "symfony/flex": "^2.10",
        "symfony/form": "8.0.*",
        "symfony/framework-bundle": "8.0.*",
        "symfony/google-mailer": "8.0.*",
        "symfony/http-client": "8.0.*",
        "symfony/http-kernel": "8.0.*",
        "symfony/intl": "8.0.*",
        "symfony/mailer": "8.0.*",
        "symfony/mime": "8.0.*",
        "symfony/monolog-bundle": "^4.0.1",
        "symfony/notifier": "8.0.*",
        "symfony/process": "8.0.*",
        "symfony/property-access": "8.0.*",
        "symfony/property-info": "8.0.*",
        "symfony/runtime": "8.0.*",
        "symfony/scheduler": "8.0.*",
        "symfony/security-bundle": "8.0.*",
        "symfony/security-csrf": "8.0.*",
        "symfony/serializer": "8.0.*",
        "symfony/stimulus-bundle": "^2.32",
        "symfony/string": "8.0.*",
        "symfony/translation": "8.0.*",
        "symfony/twig-bundle": "8.0.*",
        "symfony/ux-chartjs": "^2.32",
        "symfony/ux-dropzone": "^2.32",
        "symfony/validator": "8.0.*",
        "symfony/web-link": "8.0.*",
        "symfony/webpack-encore-bundle": "^2.4",
        "symfony/yaml": "8.0.*",
        "symfonycasts/reset-password-bundle": "^1.24",
        "symfonycasts/verify-email-bundle": "^1.18",
        "twig/cssinliner-extra": "^3.23",
        "twig/extra-bundle": "^3.23",
        "twig/intl-extra": "^3.23",
        "twig/markdown-extra": "^3.23",
        "twig/string-extra": "^3.23",
        "twig/twig": "^3.23",
        "wataridori/simple-fuzzy-search": "^0.1.1",
        "whichbrowser/parser": "^2.1.8"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*",
        "symfony/polyfill-php84": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "8.0.*",
            "docker": true
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.93.1",
        "rector/rector": "^2.3.5",
        "symfony/debug-bundle": "8.0.*",
        "symfony/maker-bundle": ">=1.65.1",
        "symfony/stopwatch": "8.0.*",
        "symfony/web-profiler-bundle": "8.0.*"
    }
}
